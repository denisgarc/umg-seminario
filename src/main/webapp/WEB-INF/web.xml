<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>app.consultas.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoDocumentoController</servlet-name>
        <servlet-class>app.consultas.controller.TipoDocumentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EspecializacionController</servlet-name>
        <servlet-class>app.consultas.controller.EspecializacionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PuestoController</servlet-name>
        <servlet-class>app.consultas.controller.PuestoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoConsultaController</servlet-name>
        <servlet-class>app.consultas.controller.TipoConsultaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoDiagnosticoController</servlet-name>
        <servlet-class>app.consultas.controller.TipoDiagnosticoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TipoTratamientoController</servlet-name>
        <servlet-class>app.consultas.controller.TipoTratamientoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoDocumentoController</servlet-name>
        <url-pattern>/TipoDocumentoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EspecializacionController</servlet-name>
        <url-pattern>/EspecializacionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PuestoController</servlet-name>
        <url-pattern>/PuestoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoConsultaController</servlet-name>
        <url-pattern>/TipoConsultaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoDiagnosticoController</servlet-name>
        <url-pattern>/TipoDiagnosticoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TipoTratamientoController</servlet-name>
        <url-pattern>/TipoTratamientoController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <display-name>Citas</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <filter>
        <filter-name>sitemesh</filter-name>
        <filter-class>org.sitemesh.config.ConfigurableSiteMeshFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>sitemesh</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
</web-app>
